@{
    var googleDocsViewerUrl = ViewData["GoogleDocsViewerUrl"] as string;
}

<!DOCTYPE html>
<html>
<head>
    <title>View PDF</title>
</head>
<body>
    @if (string.IsNullOrEmpty(googleDocsViewerUrl))
    {
        <p>No PDF URL available.</p>
    }
    else
    {
        <iframe src="@googleDocsViewerUrl" style="width:100%; height:100vh;" frameborder="0"></iframe>
    }
</body>
</html>
